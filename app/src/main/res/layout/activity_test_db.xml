<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">

        <Button
            android:id="@+id/btn_insert_list"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="插入数据"
            android:onClick="skip418"/>

        <Button
            android:id="@+id/btn_query_all"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="查询所有"
            android:onClick="skip419"/>

        <Button
            android:id="@+id/btn_clear_table"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="清空表数据"
            android:onClick="skip42"/>

        <Button
            android:id="@+id/btn_insert_new_db"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="插入新数据库"
            android:onClick="skip43"/>

    </LinearLayout>
</layout>
